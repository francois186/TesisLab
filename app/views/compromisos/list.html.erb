<style type="text/css">
    body {
        padding-top: 60px;
        padding-bottom: 40px;
    }
    .sidebar-nav {
        padding: 9px 0;
    }

    @media (max-width: 980px) {
        /* Enable use of floated navbar text */
        .navbar-text.pull-right {
            float: none;
            padding-left: 5px;
            padding-right: 5px;
        }
    }
</style>

<div class="span3">
  <div class="well sidebar-nav">
    <ul class="nav nav-list">
      <li class="nav-header">Compromisos</li>

      <% @lista.each do |comp| %>
        <li>
          <tr>
            <td><%= comp.FECHA_CREACION %></td>
            <td><%= comp.FECHA_COMPROMETIDA %></td>
            <td><%= comp.FECHA_CIERRE %></td>
          </tr>
        </li>
      <% end %>

    </ul>
  </div><!--/.well -->
</div><!--/span-->